{
  "name": "oasgraph",
  "version": "0.0.1",
  "description": "Turns APIs described by OpenAPI specifications (OAS) into GraphQL interfaces",
  "main": "lib/index.js",
  "scripts": {
    "test": "npm run build && jest",
    "api": "nodemon test/example_api_server.js",
    "dev": "DEBUG=preprocessing,translation,http nodemon --inspect test/example_gql_server.js",
    "build": "babel src/ -d lib/",
    "flow": "flow",
    "guru-load": "node test/apis_guru/load_apis_guru.js",
    "guru-test": "DEBUG=preprocessing,translation node test/apis_guru/apis_guru_test.js"
  },
  "author": "Erik Wittern",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.6.8",
    "deep-equal": "^1.0.1",
    "graphql": "^0.9.6",
    "jsonpath": "^0.2.11",
    "request": "^2.81.0",
    "swagger2openapi": "^2.3.3"
  },
  "standard": {
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "body-parser": "^1.17.2",
    "eslint-plugin-flowtype": "^2.35.0",
    "express": "^4.15.2",
    "express-graphql": "^0.6.4",
    "flow-bin": "^0.52.0",
    "fs": "0.0.1-security",
    "glob": "^7.1.2",
    "jest": "^20.0.4",
    "js-yaml": "^3.8.4",
    "nodegit": "^0.18.3",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.1",
    "yamljs": "^0.3.0"
  }
}
